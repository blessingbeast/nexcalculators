{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "calculators/calculators.json",
                    "calculators/content/**",
                    "views/**",
                    "public/**"
                ]
            }
        },
        {
            "src": "api/verify.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/google0572cb8c76265bca.html",
            "destination": "/api/verify"
        },
        {
            "source": "/(.*)",
            "destination": "/server.js"
        }
    ]
}